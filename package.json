{
  "name": "lisi-search",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "start": "cp dev.js env.js && ./node_modules/electron/dist/Electron.app/Contents/MacOS/Electron .",
    "build": "cp prod.js env.js && ./node_modules/electron-packager/cli.js . --platform=darwin --overwrite --extra-resource=node_modules --out=build"
  },
  "description": "Desktop app for searching movies and tv shows",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "redux": "^3.6.0",
    "vue": "^2.2.4",
    "vue-router": "^2.3.0"
  }
}
